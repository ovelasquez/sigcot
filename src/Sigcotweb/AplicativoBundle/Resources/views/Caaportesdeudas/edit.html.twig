{% extends '::base.html.twig' %}

{% block content %}
<h1>Caaportesdeudas edit</h1>

<form action="{{ path('aportesaprestamos_update', { 'id': entity.id }) }}" method="post" {{ form_enctype(edit_form) }}>
    {{ form_widget(edit_form) }}
    <p>
        <button type="submit">Edit</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('aportesaprestamos') }}">
            Back to the list
        </a>
    </li>
    <li>
        <form action="{{ path('aportesaprestamos_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}
{% block submenu %}
{% include 'AplicativoBundle:Caprestamos:submenu.html.twig' %}
{% endblock %}