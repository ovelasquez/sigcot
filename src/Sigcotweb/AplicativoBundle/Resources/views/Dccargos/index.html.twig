{% extends '::base.html.twig' %}

{% block content %}
<h1>Lista de cargos</h1>
<div><ul class="btns">
    <li>
        <a href="{{ path('cargoscooperativistas_new') }}">
            crear entrada
        </a>
    </li>
</ul></div>
<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Identificador</th>
            <th>Descripcion</th>
            <th>Acciones</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('cargoscooperativistas_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.identificador }}</td>
            <td>{{ entity.descripcion }}</td>
            <td>
                <ul class="btns">
                    <li>
                        <a href="{{ path('cargoscooperativistas_show', { 'id': entity.id }) }}">mostrar</a>
                    </li>
                    <li>
                        <a href="{{ path('cargoscooperativistas_edit', { 'id': entity.id }) }}">editar</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>


{% endblock %}
{% block submenu %}
{% include 'AplicativoBundle:Cooperativistas:submenu.html.twig' %}
{% endblock %}