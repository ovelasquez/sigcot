{% extends '::base.html.twig' %}

{% block content %}
<h1>Dfcconceptosaportes</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Monto</th>
            <td>{{ entity.monto }}</td>
        </tr>
        <tr>
            <th>Titulo</th>
            <td>{{ entity.titulo }}</td>
        </tr>
        <tr>
            <th>Descripcion</th>
            <td>{{ entity.descripcion }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('conceptosaportesc') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('conceptosaportesc_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('conceptosaportesc_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}
{% block submenu %}
{% include 'AplicativoBundle:Dfcaportes:submenu.html.twig' %}
{% endblock %}