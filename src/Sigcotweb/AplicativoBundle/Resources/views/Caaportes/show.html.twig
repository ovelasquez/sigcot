{% extends '::base.html.twig' %}

{% block content %}
<h1>Caaportes</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Monto</th>
            <td>{{ entity.monto }}</td>
        </tr>
        <tr>
            <th>Fecha</th>
            <td>{{ entity.fecha|date('Y-m-d H:i:s') }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('aportesdecajadeahorro') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('aportesdecajadeahorro_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('aportesdecajadeahorro_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}
{% block submenu %}
{% include 'AplicativoBundle:Caprestamos:submenu.html.twig' %}
{% endblock %}